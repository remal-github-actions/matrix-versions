{
  "$schema": "https://json.schemastore.org/package",
  "name": "matrix-versions",
  "version": "1.0.0",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/main.js",
  "scripts": {
    "generate-config": "node config.schema-update && json2ts config.schema.json > src/internal/config.ts",
    "generate": "npm run generate-config",
    "lint": "eslint --fix",
    "test": "jest --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run generate && npm run lint && rimraf build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir . --outDir build",
    "dist": "npm run compile && rimraf dist && ncc build build/src/main.js --out dist",
    "build": "npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/glob": "0.4.0",
    "ajv": "8.12.0",
    "console-log-level": "1.4.1",
    "deepmerge": "4.3.1",
    "renovate": "35.39.0",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-github": "4.7.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.5.0",
    "jest-circus": "29.5.0",
    "rimraf": "4.4.1",
    "json-schema-to-typescript": "12.0.0",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  }
}
